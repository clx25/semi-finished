{
  "get": {
    "/menu": {
      "summary": "获取所有菜单",
      "params": {
        "@tb1": "semi_menu",
        "\\": "sort",
        "%label%$$": "name",
        "#json": "icon",
        "^": {},
        ":": "route_path:routePath,route_name:routeName,table_name:tableName,is_hidden:isHidden"
      }
    },
    "/icon": {
      "summary": "获取所有图标",
      "params": {
        "@tb": "semi_icon"
      }
    }
  },
  "post": {
    "menu": {
      "summary": "修改菜单",
      "params": {
        "@tb": "semi_menu",
        "id$$": "id",
        "route_path$$": "routePath",
        "route_name$$": "routeName",
        "label$$": "label",
        "component$$": "component",
        "parent$$": "parent",
        "table_name$$": "tableName",
        "icon$$": "icon",
        "sort$$": "sort",
        "query$$": "query",
        "variable$$": "variable"
      }
    },
    "/icon": {
      "summary": "获取所有图标",
      "params": {
        "@tb": "semi_icon",
        "image$$": "image"
      }
    }
  },
  "put": {
    "menu": {
      "summary": "修改菜单",
      "params": {
        "@tb": "semi_menu",
        "id$$": "id",
        "route_path$$": "routePath",
        "route_name$$": "routeName",
        "label$$": "label",
        "component$$": "component",
        "parent$$": "parent",
        "table_name$$": "tableName$$",
        "icon$$": "icon",
        "sort$$": "sort",
        "query$$": "query",
        "variable$$": "variable"
      }
    },
    "/hidden": {
      "summary": "修改隐藏权限",
      "params": {
        "@tb": "semi_menu",
        "id$$": "id",
        "is_hidden$$": "isHidden"
      }
    }
  },
  "delete": {
    "menu": {
      "summary": "删除菜单",
      "params": {
        "@tb": "semi_menu",
        "id$$": "id"
      }
    },
    "icon": {
      "summary": "删除自定义菜单图标",
      "params": {
        "@tb": "semi_icon",
        "id$$": "id"
      }
    }
  }
}